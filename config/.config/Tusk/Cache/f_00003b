/***
 * Conventions for common-editor
 *
 * We also define some standard values, to make sure we get a consistent visual
 * experience, for example instead of <blockquote> we now use
 * <div style="margin-left:40px">
 ***/

en-note,
en-template {
  line-height: 1.45 !important;
  display: block;
}

en-note.viewmode,
en-template.viewmode {
  overflow-wrap: break-word;
}

en-note div,
en-template div {
  margin: 0;
  padding: 0;
}

body.android en-note div,
body.android en-template div {
  max-width: 100%;
}

body.android en-note span,
body.android en-template span {
  max-width: 100%;
}

en-note p,
en-template p {
  margin-top: 1em;
  margin-bottom: 1em;
}

en-note h1,
en-template h1 {
  margin: 0;
  margin-bottom: 0.67em;
}

en-note h2,
en-template h2 {
  margin: 0;
  margin-bottom: 0.83em;
}

en-note h3,
en-template h3 {
  margin-top: 1em;
  margin-bottom: 1em;
}

en-note h4,
en-template h4 {
  margin-top: 1.33em;
  margin-bottom: 1.33em;
}

en-note h5,
en-template h5 {
  margin-top: 1.67em;
  margin-bottom: 1.67em;
}

en-note h6,
en-template h6 {
  margin-top: 2.33em;
  margin-bottom: 2.33em;
}

en-note blockquote,
en-template blockquote {
  margin: 0 40px;
}

en-note ol,
en-template ol,
en-note ul,
en-template ul,
en-note dl,
en-template dl {
  margin: 0;
  padding: 0;
  padding-left: 40px;
}

en-note li,
en-template li {
  margin: 0;
  padding: 0;
}

en-note li[style*="direction:rtl"],
en-template li[style*="direction:rtl"],
en-note li[style*="direction: rtl"],
en-template li[style*="direction: rtl"] {
  /* A fix for the RTL bug in WebKit */
  margin-right: 40px;
  margin-left: -40px;
  direction: rtl;
}

en-note th,
en-template th {
  font-weight: bold;
  text-align: left;
}

en-note hr,
en-template hr {
  /* Horizontal rule is visually defined */
  margin: 1px 0px;
  padding-top: 1px;
  border: none;
  border-width: thick 0px;
  border-style: solid none;
  border-color: white;
  background-color: #999;
}

en-note hr.selected,
en-template hr.selected {
  border-color: lightblue;
}

en-note mark,
en-template mark {
  background-color: #fffaa5;
}

en-note del,
en-template del {
  /* Make sure this is displayed correctly */
  text-decoration: line-through;
}

en-note img:not(.en-media-placeholder):not(.en-richlink-placeholder),
en-template img:not(.en-media-placeholder):not(.en-richlink-placeholder) {
  /* Images should not exceed the editor's width */
  max-width: 100%;
  height: auto;
}

en-note a[href^="evernote:"],
en-template a[href^="evernote:"] {
  color: #69AA35;
}

